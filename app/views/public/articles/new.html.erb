<div class="container">
<h1>新規投稿</h1>

<%= form_with model: @article, url: '/articles', method: :post do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>
  
  <h4>本文</h4>
  <%= f.text_area :content %>
  <!-- 追加部分 -->
  <h4>画像</h4>
  <%= f.file_field :images, multiple: true, accept: "image/*" %>
  <%= f.submit '投稿' %>
  
<% end %>
</div>